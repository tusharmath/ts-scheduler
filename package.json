    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "mocha": "^6.1.4",
    "prettier": "^1.17.0",
    "semantic-release": "^15.13.3",
    "travis-deploy-once": "^5.0.11",
